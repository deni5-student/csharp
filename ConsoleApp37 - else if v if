using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp37
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name = "Frans";

            if (name == "Frans")
            {
                Console.WriteLine("Hello Frans");
                name = "Kenneth";

            }

            if (name == "Kenneth")
            {
                Console.WriteLine("Hello Kenneth");
            }

        }
    }
}
